#include "main.h"
#include <stdio.h>

/**
 * print_to_98 - print natural number
 *@n: number to be use
 *Return: Always 0
 */
void print_to_98(int n)
{
	if (n <= 98)
	{
		for (i = n; i <= 98; i++)
		{
			printf("%d", i);
			if (n != 98)
				printf(", ");
		}
	}

	if else (n == 98)
		{
			printf("n");
		}
	else (n >= 98)
	     {
		     for (i = n; i >= 98; i--)
		     {
			     printf("%d", i);
			     if (i != 98)
			     {
				     printf(", ");
			     }
		     }
	     }
	printf("\n");
}
